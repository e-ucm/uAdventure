  <Product Id="4bbf00d3-afb7-4fdb-a987-3bf10df0c224" Name="Setup" Language="1033" Codepage="Windows-1252" Version="1.0.0.0" UpgradeCode="4bbf00d3-afb7-4fdb-a987-3bf1fdf0c223" Manufacturer="oleg.shilo">
